TopHeader.getHotcateList = function(cateItemsIdx)
{ 
	var cateItems = hotSubCateMenu[cateItemsIdx];
	var retValue = "";
	var subMenuIdx = 0;
	
	retValue += '<div class="in-line"> ';
	retValue += ' <div class="catekey-sub"> ';  
	//debugger
	for(var idx1 = 0; idx1< cateItems.menu.length; idx1++)
	{
		retValue += '		<ul class="cateSList"> '; 
		retValue += '			<li>    ';
		if(cateItems.menu[idx1].subMenu != null && typeof(cateItems.menu[idx1].subMenu) != 'undefined'  )
		{
			subMenuIdx++;
				
			if($.browser.msie )
			{
				retValue += "				<a href=\""+ TopHeader.getLinkUrl(cateItems.menu[idx1].categoryCode) +"\" target=\"_top\"   onMouseleave=\"hotcateOut('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"');\"><span>"+cateItems.menu[idx1].menuName +"</span></a>";
			}
			else
			{
				retValue += "				<a href=\"" + TopHeader.getLinkUrl(cateItems.menu[idx1].categoryCode) + "\" target=\"_top\"  onMouseOut=\"hotcateOut('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"');\"><span>"+cateItems.menu[idx1].menuName +"</span></a>";				
			}
			
			retValue += TopHeader.getHotcateSubList(cateItems.menu[idx1].subMenu,cateItemsIdx, subMenuIdx);
		}
		else
		{
		
			retValue += '				<a href="'+ TopHeader.getLinkUrl(cateItems.menu[idx1].categoryCode) +'" target=\"_top\" ><span>'+cateItems.menu[idx1].menuName +'</span></a>';			
		}
		
		retValue += '				<ul class="cSList">';
		if(cateItems.menu[idx1].middleMenu != null && typeof(cateItems.menu[idx1].middleMenu) != 'undefined'  )
		{
			for( var idx2 = 0; idx2< cateItems.menu[idx1].middleMenu.length; idx2++)
			{
				// middleMenu
				if(cateItems.menu[idx1].middleMenu[idx2].isPoint != 'false')
				{
					retValue += '	<li class="point">';
				}
				else
				{
					retValue += '	<li>';
				}
				
				spanClass = "";
				
				if(cateItems.menu[idx1].middleMenu[idx2].isNew == 'true')
				{
					spanClass += 'ico_n';
				}			
				
				if(cateItems.menu[idx1].middleMenu[idx2].isHit == 'true')
				{
						spanClass += " ico_h";
				}
				
				//var lpPage3 = "http://finding.about.co.kr/Category/List.aspx?DisplayCategories=" + cateItems.menu[idx1].middleMenu[idx2].categoryCode;
				var middleCateCode = cateItems.menu[idx1].middleMenu[idx2].categoryCode;
				var middleLinkUrl = "";
				if( cateItems.menu[idx1].middleMenu[idx2].linkUrl != "")
				{
					middleLinkUrl = cateItems.menu[idx1].middleMenu[idx2].linkUrl;
				}
				else
				{
					middleLinkUrl = TopHeader.getLinkUrl(middleCateCode);
				}
				
				if(cateItems.menu[idx1].middleMenu[idx2] != null && typeof(cateItems.menu[idx1].middleMenu[idx2]) != 'undefined'  )
				{
					// 서브 메뉴가 있는 경우
					subMenuIdx++;
					if($.browser.msie )
					{
						//retValue += "				<a href=\""+ lpPage1 +"\" target=\"_top\"   onMouseleave=\"hotcateOut('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"');\"><span>"+cateItems.menu[idx1].menuName +"</span></a>";
						retValue += "	<a href=\"" + middleLinkUrl + "\" target=\"_top\" onMouseleave=\"hotcateOut('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"');\"    ><span class=\"" + spanClass + "\">";
					}
					else
					{
						//retValue += "				<a href=\"" + lpPage1 + "\" target=\"_top\"  onMouseOut=\"hotcateOut('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"');\"><span>"+cateItems.menu[idx1].menuName +"</span></a>";				
						retValue += "	<a href=\"" + middleLinkUrl + "\" target=\"_top\" onMouseOut=\"hotcateOut('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"');\"    ><span class=\"" + spanClass + "\">";
					}
					retValue += cateItems.menu[idx1].middleMenu[idx2].menuName+'</span></a>';
					retValue += TopHeader.getHotcateSubList(cateItems.menu[idx1].middleMenu[idx2].subMenu,cateItemsIdx, subMenuIdx);;
			  }
			  else
			  {
			  	retValue += "	<a href=\"" + TmiddleLinkUrl + "\" target=\"_top\" ><span class=\"" + spanClass + "\">";
			  	retValue += cateItems.menu[idx1].middleMenu[idx2].menuName+'</span></a>';			  	
			  }
			  
				retValue += '		</li>';			
			}
		}
		retValue += '				</ul>';
		retValue += '			</li>';
		retValue += '		</ul> '; 	
	}	 
	
	retValue += ' </div>';
	retValue += '	<!-- catekey-sub End -->';
	var serviceMenu = serviceItems[cateItemsIdx].serviceItem;
	var serviceType = serviceItems[cateItemsIdx].serviceType;
	retValue += TopHeader.getHotcateServiceList(serviceMenu,serviceType);
	
	aboutCategoryAD2Banner = TopHeader.getAboutCategoryAD2Banner(cateItemsIdx);
	if(aboutCategoryAD2Banner != null)
	{
		retValue += '<div class="cate_bnr">';
		retValue += '	<a href="'+ aboutCategoryAD2Banner.linkUrl +'" target="_blank" >';
		if(locationProtocol == "http://")
		{
			//retValue += "<a href=\"" + rWingBanner.linkUrl +"\" target=\"_blank\"> <img src='" + locationProtocol + "i.iabout.kr/w/"  +rWingBanner.imageUrl +"'  width='80' height='250' > </a>";		
			if(headerType == "H")
			{
				retValue += '		<img alt="'+ aboutCategoryAD2Banner.title +'" src="'  + locationProtocol + "i.iabout.kr/w/"  +aboutCategoryAD2Banner.imageUrl1 +'" width="153" height="56" >';
			}
			else
			{
				retValue += '		<img alt="'+ aboutCategoryAD2Banner.title +'" src="'  + locationProtocol + "i.iabout.kr/w/"  +aboutCategoryAD2Banner.imageUrl2 +'" width="141" height="56" >';
			}
				
		}
		else
		{
			//retValue += "<a href=\"" + rWingBanner.linkUrl +"\" target=\"_blank\"> <img src='" + locationProtocol + "is.iabout.kr/w/"  +rWingBanner.imageUrl +"'  width='80' height='250'  > </a>";					
			if(headerType == "H")
			{
				retValue += '		<img alt="'+ aboutCategoryAD2Banner.title  +'" src="'  + locationProtocol + "is.iabout.kr/w/"  +aboutCategoryAD2Banner.imageUrl1 +'" width="153" height="56" >';
			}
			else
			{
				retValue += '		<img alt="'+ aboutCategoryAD2Banner.title  +'" src="'  + locationProtocol + "is.iabout.kr/w/"  +aboutCategoryAD2Banner.imageUrl2 +'" width="141" height="56" >';				
			}
		}
		
		retValue += '	</a> ';
		retValue += '</div>	';			
	}
	
	
	//retValue += '<div class="cate_bnr">';
	//retValue += '	<a href=" http://promotion.about.co.kr/EventAbout.aspx?eventID=201188173921" target="_top" >';
	//retValue += '		<img alt="HP" src="http://pics.about.co.kr/os2/main/hp_110904_137x44.gif">';
	//retValue += '	</a> ';
	//retValue += '</div>	';			
	
	
	retValue += '</div>';  
                       
	return retValue;		
}

TopHeader.getAboutCategoryAD2Banner = function(locationType )
{
	if(typeof(gAboutCategoryAD2Banner) != 'undefined' && gAboutCategoryAD2Banner.length > 0)
	{
		for(i = 0; i< gAboutCategoryAD2Banner.length ; i++)
		{
			if(gAboutCategoryAD2Banner[i].locationType  == locationType)
			{
				return gAboutCategoryAD2Banner[i];
			}
		}
	}
	return null;
}

TopHeader.getHotcateSubList = function(subItems, cateItemsIdx , subMenuIdx)
{ 
	var retValue = "";
	if(typeof(subItems) !=  'undefined' && subItems != null)
	{
		if(subItems.length > 0)
		{
			if($.browser.msie )
			{
				retValue +="<div class=\"cateSList-sub\" id='cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"'  onMouseleave=\"hotcateOut('cateSList-sub_" + cateItemsIdx + "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx   +"');\"       > ";
			}
			else
			{
				retValue +="<div class=\"cateSList-sub\" id='cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx +"'  onMouseOut=\"hotcateOut('cateSList-sub_" + cateItemsIdx + "_" + subMenuIdx  +"');\" onMouseOver=\"hotcateOver('cateSList-sub_"+cateItemsIdx+ "_" + subMenuIdx   +"');\"       > ";			
			}
			
			if(subItems.length > 12)
			{ 
				retValue +='	<div class="box" style="width:170px;" > ';
			}
			else if(subItems.length == 12)
			{
				//if( $browser.version == "8.0" && $browser.msie)
				//{
				//	retValue +='	<div class="box"> ';
				//}
				if($.browser.version != "8.0" && $.browser.msie)
				{
					retValue +='	<div class="box" style="width:170px;" > ';					
				}
				else
				{
					retValue +='	<div class="box" > ';										
				}
									
			}			
			else
			{
				retValue +='	<div class="box" > ';				
			}
			retValue +='		<ul class="sin-line"> ';
			for(var idx = 0 ; idx < subItems.length; idx++)
			{
				retValue +='<li>';
				var linkUrl1 = TopHeader.getLinkUrl(subItems[idx].categoryCode); 
				
				retValue +=' <a href="'+ linkUrl1 +'"><span>' + subItems[idx].menuName  + '</span></a>';
				if( subItems[idx].keyword != null && subItems[idx].keyword != "")
				{
					if(subItems[idx].keyword.startsWith("http://"))
					{
						var linkUrl2 = subItems[idx].keyword;						
					}
					else
					{
						var linkUrl2 = srpPage + subItems[idx].keyword;						
					}
					retValue += '<a href="' + linkUrl2 +'" title="검색" class="qcate" target="_top" ></a>';
				}

				retValue +='</li> ';
			}
			retValue +='		</ul> ';
			retValue +='	</div> ';
			retValue +='</div> ';
		}
	}
	return retValue;


}

