function shBannerChange() {
	var istop = GetUrlParamVal("istop");
	if (istop != "y") {
		var tnum;
		if(typeof(gAboutSearchAD) != 'undefined' && gAboutSearchAD.length>0)
		{
			tnum = Math.floor(Math.random() * gAboutSearchAD.length);
			//$("#htxtHeaderSearch").css("color", shBanners[tnum][2]);
			$("#htxtHeaderSearch").css("color", 'rgb(98, 106, 217)' );
			$("#htxtHeaderSearch").css("font-family", "Dotum,돋움,Gulim,굴림");
			$("#htxtHeaderSearch").css("font-style", "normal");
			$("#htxtHeaderSearch").css("font-variant", "normal");
			$("#htxtHeaderSearch").css("font-weight", "normal");
			$("#htxtHeaderSearch").css("font-size", "12px");
			$("#htxtHeaderSearch").css("line-height", "30px");
			$("#htxtHeaderSearch").css("font-size-adjust", "none");
			$("#htxtHeaderSearch").css("font-stretch", "normal");
			$("#htxtHeaderSearch").css("-x-system-font", "none");
			$("#htxtHeaderSearch").val(gAboutSearchAD[tnum].title);
	
			$("#hbtnHeaderSearch").unbind("click");
			$("#hbtnHeaderSearch").click(function() {
					window.open(gAboutSearchAD[tnum].linkUrl,'','');			
			});
		}
		else
		{
			tnum = Math.floor(Math.random() * shBanners.length);
			$("#htxtHeaderSearch").css("color", shBanners[tnum][2]);
			$("#htxtHeaderSearch").css("font-family", "Dotum,돋움,Gulim,굴림");
			$("#htxtHeaderSearch").css("font-style", "normal");
			$("#htxtHeaderSearch").css("font-variant", "normal");
			$("#htxtHeaderSearch").css("font-weight", "normal");
			$("#htxtHeaderSearch").css("font-size", "12px");
			$("#htxtHeaderSearch").css("line-height", "30px");
			$("#htxtHeaderSearch").css("font-size-adjust", "none");
			$("#htxtHeaderSearch").css("font-stretch", "normal");
			$("#htxtHeaderSearch").css("-x-system-font", "none");
			$("#htxtHeaderSearch").val(shBanners[tnum][3]);

        	/*	
            // 20120126 어바웃 소개 페이지 삭제로 링크 삭제
            
			$("#hbtnHeaderSearch").unbind("click");
			$("#hbtnHeaderSearch").click(function() {
			
			
			if (shBanners[tnum][1] != "open") {
			top.document.location.href = shBanners[tnum][0];
			}
			else {
			window.open(shBanners[tnum][0],'','scrollbars=no,fullscreen=yes,toolbar=no,directories=yes,status=yes,status=yes,top=0,resizable=no');
	
				}
			
			});
			*/
		}
	}
} 



