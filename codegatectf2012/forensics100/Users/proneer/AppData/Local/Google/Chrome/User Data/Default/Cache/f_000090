// 인기 검색어
var searchRanking = '';

// 인기 검색어 Link 생성
GenSearchRankingStr();

// 버튼 이벤트
$(document).ready(function()
{
	var $f = $("#suggestIframe_ass");
	
	if(locationProtocol == "http://")
	{
		if( typeof($f[0]) != 'undefined')
		{
			var fd = $f[0].contentWindow;
				
			// 검색어 이벤트 처리.. 
			$("#htxtHeaderSearch").keyup(
				function(e){
					if(typeof(fd.txtHeader_Keyup)!="undefined")
						fd.txtHeader_Keyup(e);  // run function
				}
			)
		}
	}
	
	$('#htxtHeaderSearch').FFInputBoxFix("suggestIframe_ass");
	//if (jQuery.browser.mozilla == true){	$('#htxtHeaderSearch').focusFunc(); };
	
	$("#hbtnHeaderSearch").click(function()
	{
		TopHeader.hbtnHeaderSearchClick();
	});

	$("#htxtHeaderSearch").focus(function(e)
	{
		var istop = GetUrlParamVal("istop");

		if (istop.toLowerCase() != "y") {
			$("#htxtHeaderSearch").val("");
		}

		$("#htxtHeaderSearch").css("color", "");
		$("#htxtHeaderSearch").css("font-family", "");
		$("#htxtHeaderSearch").css("font-style", "");
		$("#htxtHeaderSearch").css("font-variant", "");
		$("#htxtHeaderSearch").css("font-weight", "");
		$("#htxtHeaderSearch").css("font-size", "");
		$("#htxtHeaderSearch").css("line-height", "");
		$("#htxtHeaderSearch").css("font-size-adjust", "");
		$("#htxtHeaderSearch").css("font-stretch", "");
		$("#htxtHeaderSearch").css("-x-system-font", "");

		$("#hbtnHeaderSearch").unbind("click");
		$("#hbtnHeaderSearch").click(function()
		{
			TopHeader.hbtnHeaderSearchClick();
		});
	});

	$("#htxtHeaderSearch").keyup(function(e)
	{
		var searchValue = jQuery.trim($("#htxtHeaderSearch").val());

		if (e.keyCode == 13) {
			TopHeader.hbtnHeaderSearchClick();
		}		
	});

	$("#htxtHeaderSearch").click(function(event)
	{
		// To Do : 검색창 클릭시 검색어를 삭제하지 않고 전체선택을 해줌.
		//         focus -> click순으로 일어나 focus에서 선택을 해놓으면 safari계열에서 선택이 풀리는 경향이 있어 click에 검색을 선택하도록 함.
//		keyword = $.trim(this.value); 

//		if (keyword != "" && keyword.length > 0) {
//			this.select();
//		}

		return false;
	});

	// document 를 클릭시 사이즈를 줄여준다.
	$(document).click(function(obj)
	{
		if (document.getElementById("suggestIframe_ass") != null) {
			document.getElementById("suggestIframe_ass").style.height = "0px";
		}
	});

	if(headerType != "S")
	{
		shBannerChange();
	}

});

// 인기 검색어 Link 생성
function GenSearchRankingStr()
{
	try
	{
		for(i = 0; i < keywordRankingJSON.RankingItem.length; i++)
		{
			if(searchRanking.length == 0)
			{
				searchRanking = GenLinkStr(keywordRankingJSON.RankingItem[i].keyword);
			}
			else
			{
				searchRanking += ", " +  GenLinkStr(keywordRankingJSON.RankingItem[i].keyword);
			}

		}
	}
	catch (e)
	{
		searchRanking = '';
	}

}

// 인기검색어 Link 문자열 생성
function GenLinkStr(keyword)
{
	var returnValue = "<a href='" + "http://finding.about.co.kr/Search/Search.aspx?istop=y&Keyword=" + encodeURIComponent(keyword) + "' target='_top'>" + keyword + "</a>";
	return returnValue;
}

// 즐겨찾기 추가
function addfavorite() {
	window.external.AddFavorite('http://www.about.co.kr', 'about');
}

function TopHeader() { }

TopHeader.hbtnHeaderSearchClick = function() {
	var searchValue = jQuery.trim($("#htxtHeaderSearch").val());

	if (searchValue.length > 0) {
		top.document.location.href = "http://finding.about.co.kr/Search/Search.aspx?istop=y&Keyword=" + encodeURIComponent(searchValue);
	}
	else {
		alert("검색어를 입력해주세요");
		$("#htxtHeaderSearch").focus();
		return false;
	}
}


///////////////////////////////////////////////////////////////////////////////////////////////////////////
//검색키워드값 추출(url 내 istop = y 파라메터로 헤더에서 키워드 검색여부 추측)
function GetUrlParamVal(name) {

	var keyWord = "";
	try {
		var keyWordname = (name != "") ? name : "keyword";

		var isTop = false;
		var urlString = new String(window.location);
		var markLocation = urlString.search(/\?/);

		if (markLocation != -1) {

			urlString = urlString.substr(markLocation + 1);
			var paramArray = urlString.split(/&/g); 
			for (var i = 0; i < paramArray.length; i++) {

				var arrayValue = paramArray[i].split(/=/);
				if ((keyWordname == "istop") && (arrayValue[0].toLowerCase() == keyWordname.toLowerCase())) {
					keyWord = arrayValue[1].toLowerCase().replace("#keyword", "");
				}
				else if ((keyWordname != "istop") && (arrayValue[0].toLowerCase() == keyWordname.toLowerCase())) {
					//keyWord = decodeURIComponent(arrayValue[1].toLowerCase()).replace("#keyword", "");  // 공백이들어갈 자리에 + 기호가 나오는 현상 보정 by 이동열(2011.1.4)
					keyWord = decodeURIComponent(arrayValue[1].toLowerCase().replace(/\+/g, ' ')).replace("#keyword", ""); 
				} 
			}
		}
	}
	catch (e) { 
		keyWord = "";
	}

	return keyWord;
}
 
///////////////////////////////////////////////////////////////////////////////////////////////////////////

TopHeader.setNavigationTab = function(headerType) {

    //검색어Search와 자동완성기능(Suggest.htm)에서 쓰임 
    var istop = GetUrlParamVal("istop");
    var keyWord = ((istop == "y") ? GetUrlParamVal("keyword") : "");

    //0건 검색결과 재검색에 대한 키워드 처리.
    if (GetUrlParamVal("isZeroResultRetry") == "true") {
        keyWord = GetUrlParamVal("userTypedKeyword");
    }

    var retValue = '';
    var loginLinkUrl = "";

    if (location.href.toLowerCase().indexOf("member.about.co.kr/login") < 0)	//로그인 후 로그인 페이지로 Redirect되는 것을 방지.
    {
        loginLinkUrl = encodeURIComponent(location.href);
    }
    else {
        loginLinkUrl = "";
    }

    // 모든페이제서 공통 헤더 스크립트를 쓴다.. 
    // 분기탈 내용이 있으면 (headerType == "H" --> Home , headerType == "H" --> PCP , headerType == "A" --> otherPage로 세팅)

    // 실제 날보다 하루 많게 세팅했음.. 주의
    // 그리고 new Date()는 한달 적게 나옵니다. 주의 하시길.. 
    var nowYear = new Date().getFullYear();
    var nowMonth = new Date().getMonth() + 1;
    var nowDay = new Date().getDate();
    var dateTimeNow = new Date();

    dateTimeNow.setFullYear(nowYear, nowMonth, nowDay);

    var worldcupFinal = new Date().setFullYear(2010, 7, 12);

    retValue += '<div id="headerSZ3">';
    retValue += '	<div class="hsz_in">';
    retValue += '		<div class="hsz_inner" id="hsz_inner">';
    retValue += '			<!-- IE 이외의 브라우저 플래시 플러그인 설치 알림 -->';
    retValue += '			<div id="flashPluginContainer" style="position:absolute; width:0; height:0px; left:0px; top:0px; border:0px solid black;">';
    retValue += '				<embed src="' + locationProtocol + 'pics.about.co.kr/os2/flash/flashPlugin.swf" wmode="transparent" quality="high" width="0" height="0" name="flashPluginSwf" align="middle" allowScriptAccess="always" allowFullScreen="false" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" />';
    retValue += '			</div>';
    retValue += '			<!-- IE 이외의 브라우저 플래시 플러그인 설치 알림 -->';
    retValue += '			<!-- topN Start -->';
    retValue += '			<div class="topN">';

    retValue += '		<div id="aboutLogoDiv" class="logoArea" > ';
    retValue += '			<h1>about shopping search</h1>  ';

    //alert(aboutCiBanner);
    if (typeof (aboutCiBanner) == 'undefined') {
        retValue += '	<div class="ci_area" id="ci_img">\n';
        retValue += ' <a href="http://www.about.co.kr"><img src="' + locationProtocol + 'pics.about.co.kr/os2/common/header_sz3/logo_big.gif" alt="about" title="" /></a>';
        retValue += ' </div>';
    }
    else {
        if (aboutCiBanner.imageType == "0") {
            retValue += '	<div class="ci_area" id="ci_img">\n';
            retValue += ' <a href="http://www.about.co.kr"><img src="' + locationProtocol + aboutCiBanner.imageUrl.replace('http://', '') + '" alt="about" title="" /></a>';
            retValue += ' </div>';

        }
        else if (aboutCiBanner.imageType == "1") {
            retValue += '	<div class="ci_area" id="ci_flash">\n';
            retValue += '	    <script type="text/javascript"> \n';
            retValue += '     var ciflashvars={ \n';
            retValue += '     // link  :  배너 클릭 시 이동할 링크 url \n';
            retValue += '     // target : 배너 클릭 시 이동할 링크 target \n';
            retValue += '     link : \'http://www.about.co.kr\', \n';
            if (headerType == "H") {
                retValue += '		type : "main",';
            }
            else {
                retValue += '		type : "sub",';
            }
            retValue += '     target : \'_top\' \n';
            retValue += '     }; \n';
            retValue += "     embedFlash('" + locationProtocol + aboutCiBanner.imageUrl.replace('http://', '') + "', 'ci_flash', '100%', '100%', \"opaque\", ciflashvars); \n";
            retValue += '    <\/script>\n';
            retValue += '   <\/div>\n';
        }
        else {
            var hCiHtml = aboutCiBanner.htmlContents.replace("[%=이미지%]", locationProtocol + aboutCiBanner.imageUrl.replace('http://', ''));
            hCiHtml = hCiHtml.replace("[%=링크%]", aboutCiBanner.linkUrl);
            retValue += hCiHtml;
        }
    }
    retValue += '							</div> ';



    retValue += '				<!-- topmenuN Start -->                                                                 ';
    retValue += '				<div class="topmenuN">                                                                  ';
    if (!loginCheck) {
        retValue += '	<ul> ';
        retValue += '		<li class="login"><a href="https://member.about.co.kr/login/login.aspx?next_url=' + loginLinkUrl + '" target="_top" ><span class="vNone">로그인</span></a></li>  ';
        retValue += '		<li><a href="https://member.about.co.kr/SignUp/MemberJoinIntro.aspx" target="_top" >회원가입</a></li> ';
        retValue += '		<li><a href="http://member.about.co.kr/MyBuy/default.aspx" target="_top" >about me</a></li> ';
        retValue += '		<li class="last"><a href="http://member.about.co.kr/CustomerCenter/Default.aspx" target="_top" >고객센터</a></li> ';
        retValue += '	</ul> ';
    }
    else {
        retValue += '	<ul id="aboutLoginInfo"> ';
        retValue += '		<li class="logout"><a href="http://member.about.co.kr/login/logout.aspx" target="_top" ><span class="vNone">로그아웃</span></a></li> ';
        retValue += '		<li><a href="https://member.about.co.kr/MyBuy/MyInfo/MyInfoConfirmPwd.aspx" target="_top">회원정보수정</a></li> ';
        retValue += '		<li><a href="http://member.about.co.kr/MyBuy/default.aspx" target="_top">about me</a></li> ';
        retValue += '		<li class="last"><a href="http://member.about.co.kr/CustomerCenter/Default.aspx" target="_top">고객센터</a></li> ';
        retValue += '	</ul> ';
        retValue += '<iframe src="' + locationProtocol + 'member.about.co.kr/login/GetMembIdForHeader.aspx" style="display:none"></iframe>'
    }

    retValue += '				</div>                                                                                  ';
    retValue += '				<!-- topmenuN End -->                                                                   ';
    retValue += '				<!-- browserOpt Start -->                                                               ';
    retValue += '				<div class="browserOpt">                                                                ';
    retValue += '					<dl>                                                                                  ';
    retValue += '						<dt><span class="vNone">어바웃</span></dt>                                          ';
    retValue += '						<dd class="hotkey"><a onclick="HeaderCorner.shortCut();" style="cursor:pointer"><span class="vNone">바로가기</span></a></dd>         ';
    retValue += '						<dd class="bookmarker"><a onclick="HeaderCorner.addFavorite()" style="cursor:pointer;" ><span class="vNone">즐겨찾기</span></a></dd>     ';
    retValue += '					</dl>                                                                                 ';
    retValue += '				</div>																																									';
    retValue += '				<!-- browserOpt End -->';
    retValue += '				<!-- 검색바 -->';
    retValue += '				<div class="searchArea">';
    retValue += '					<fieldset>';
    retValue += '						<legend>검색하기</legend>';
    retValue += '						<input type="text" id="htxtHeaderSearch" autocomplete="off" name="htxtHeaderSearch" class="input-box" value="' + keyWord + '" style="ime-mode:active;" /><!-- style="color:#000;font:bold 14px/1 Dotum,돋움,Gulim,굴림;" --> ';
    retValue += '						<img id="hbtnHeaderSearch" name="btnHeaderSearch" src="' + locationProtocol + 'pics.about.co.kr/os2/common/header_sz3/searchB_btn.gif" class="btn" alt="Search" title="검색"  style="cursor:pointer;"  /> ';
    retValue += '					</fieldset> ';
    retValue += '					<!-- 자동완성기능 --> ';
    if (locationProtocol == "http://") {
        retValue += '						<div id="suggestionLayer_ass" headerType="' + headerType + '" ><iframe id="suggestIframe_ass"    class="drop-layer" style="height:0px;" scrolling="no" frameborder="0" marginwidth="0" marginheight="0" allowtransparency="true" src="http://finding.about.co.kr/Search/suggest.htm"></iframe></div>';
    }
