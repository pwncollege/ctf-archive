// 로그인 팝업 띄위기
TopHeader.loginPopUp= function()
{
	var popUrl = 'https://member.about.co.kr/Login/LoginPop.aspx';//?next_url=?next_url=' + linkurl; 
	window.open(popUrl, "", "toolbar=no,location=yes,directories=no,status=no,menubar=no,scrollbars=no, resizable=no,copyhistory=no ,width=1188 height=880, left=10,top=10");
	
}


  
TopHeader.setSocialShoppingGNB = function(headerType) {

	//검색어Search와 자동완성기능(Suggest.htm)에서 쓰임 
	var istop = GetUrlParamVal("istop");
	var keyWord = ((istop == "y") ? GetUrlParamVal("keyword") : "");
	

	var retValue = '';
	var loginLinkUrl = "";

	if (location.href.toLowerCase().indexOf("member.about.co.kr/login") < 0)	//로그인 후 로그인 페이지로 Redirect되는 것을 방지.
	{
		loginLinkUrl = encodeURIComponent(location.href);
	}
	else {
		loginLinkUrl = "";
	}
	
	
	retValue += '		<!-- socialTop Start --> ';
	retValue += '		<div id="socialTop"> ';
	retValue += '			<div class="inner"> ';
	retValue += '				<h1><a href="http://www.about.co.kr"><img src="' + locationProtocol + 'pics.about.co.kr/os2/social_cms/logo_about.gif" alt="about" /></a></h1> ';
	retValue += '				<div class="searchArea"> ';
	retValue += '					<fieldset> ';
	retValue += '						<legend>어바웃 검색하기</legend> ';
	retValue += '						<input type="text"  id="htxtHeaderSearch" autocomplete="off" name="htxtHeaderSearch" class="input-text" /> ';
	retValue += '						<img  id="hbtnHeaderSearch" name="btnHeaderSearch" src="' + locationProtocol + 'pics.about.co.kr/os2/social_cms/btn_search.gif" alt="Search" title="검색"  style="cursor:pointer;"  /> ';
	retValue += '					</fieldset> ';
	retValue += '				</div> ';
	retValue += '				<div class="topmenuN"> ';
	if (!loginCheck) {
		//retValue += '	<ul> ';
		//retValue += '		<li class="login"><a href="https://member.about.co.kr/login/login.aspx?next_url=' + loginLinkUrl + '" target="_top" ><span class="vNone">로그인</span></a></li>  ';
		//retValue += '		<li><a href="https://member.about.co.kr/SignUp/MemberJoinIntro.aspx" target="_top" >회원가입</a></li> ';
		//retValue += '		<li><a href="http://member.about.co.kr/MyBuy/default.aspx" target="_top" >about me</a></li> ';
		//retValue += '		<li class="last"><a href="http://member.about.co.kr/CustomerCenter/Default.aspx" target="_top" >고객센터</a></li> ';
		//retValue += '	</ul> ';
		retValue += '			<ul> ';
		retValue += '				<li class="login"><a style="cursor:pointer;" onclick="TopHeader.loginPopUp()" target="_blank" >로그인</a></li> ';
		retValue += '				<li><a href="https://member.about.co.kr/SignUp/MemberJoinIntro.aspx" target="_top" >회원가입</a></li> ';
		retValue += '				<li><a href="http://member.about.co.kr/CustomerCenter/Default.aspx" target="_top" >고객센터</a></li> ';
		retValue += '			</ul> ';
	}
	else {
		retValue += '	<ul id="aboutLoginInfo"> ';
		retValue += '		<li class="logout"><a href="http://member.about.co.kr/login/logout.aspx" target="_top" >로그아웃</a></li> ';
		retValue += '		<li><a href="https://member.about.co.kr/MyBuy/MyInfo/MyInfoConfirmPwd.aspx" target="_top">회원정보수정</a></li> ';
		retValue += '		<li><a href="http://member.about.co.kr/MyBuy/default.aspx" target="_top">about me</a></li> ';
		retValue += '		<li class="last"><a href="http://member.about.co.kr/CustomerCenter/Default.aspx" target="_top">고객센터</a></li> ';
		retValue += '	</ul> ';
		retValue += '<iframe src="' + locationProtocol + 'member.about.co.kr/login/GetMembIdForHeader.aspx" style="display:none"></iframe>'
	}
	
	retValue += '				</div> ';
	retValue += '			</div> ';
	retValue += '		</div> ';                  
	retValue += '		<!-- socialTop End --> ';	
    return retValue;
}




TopHeader.getSubDiv = function( cateItem , linkType)
{ 
	//"00" --> lpPage , "25" --> departmentPage  , '18' --> luxuryPage , 20 --> overSeaPage
		if(typeof(linkType) == 'undefined' || linkType == '')
			linkType = "00";

		var retValue = new String();
		// 서브 카테 시작 
		retValue += '<div class="cateSList-sub">';
		retValue += '<div class="box">';
		retValue += '<div class="outline top"></div>';
		retValue += '<ul class="sin-line">';
		for (idx = 0; idx < cateItem.length; idx++) { 
			//alert(cateItem[idx][2]);
			retValue += "<li>";
			if( linkType == "25")
			{
				retValue += '<a href="' + departmentPage + cateItem[idx][0] +'" target="_top" ><span>' + cateItem[idx][1] + '</span></a>';
			}
			else if( linkType == "18")
			{
				retValue += '<a href="' + luxuryPage + cateItem[idx][0] +'" target="_top" ><span>' + cateItem[idx][1] + '</span></a>';
			}
			else if( linkType == "20")
			{
				retValue += '<a href="' + overSeaPage + cateItem[idx][0] +'" target="_top" ><span>' + cateItem[idx][1] + '</span></a>';
			}			
			else
			{
				retValue += '<a href="' + lpPage + cateItem[idx][0] +'" target="_top" ><span>' + cateItem[idx][1] + '</span></a>';
			}
			if( cateItem[idx][2] != null && cateItem[idx][2] != "")
			{
				if(!cateItem[idx][2].startsWith("http://"))
				{
					retValue += '<a href="' + srpPage + cateItem[idx][2] +'" title="검색" class="qcate" target="_top" ></a>';
				}
				else
				{
					retValue += '<a href="' + cateItem[idx][2] +'" title="검색" class="qcate" target="_top" ></a>';
				}
					
			}
			 
			retValue += "</li>";
			
		}

		retValue += '</ul> ';
		
		
		//alert('11');
		//if(linkType == '02')
		//{
			
			//var dateTimeNow = new Date();
	
			// new Date생성시 한달이 많게 나옴
			//var adEndDate = new Date(2011, 10 - 1 , 1, 0, 0, 0);
			
			//if(dateTimeNow < adEndDate)
			//{
			aboutCategoryAD1Banner = TopHeader.getAboutCategoryAD1Banner(linkType);
			if(aboutCategoryAD1Banner != null)
			{
				retValue += '<div class="cate_bnr">';
				retValue += '	<a href="'+ aboutCategoryAD1Banner.linkUrl +'" target="_top" >';
				if(locationProtocol == "http://")
				{
					//retValue += "<a href=\"" + rWingBanner.linkUrl +"\" target=\"_blank\"> <img src='" + locationProtocol + "i.iabout.kr/w/"  +rWingBanner.imageUrl +"'  width='80' height='250' > </a>";		
					retValue += '		<img alt="'+ aboutCategoryAD1Banner.title +'" src="'  + locationProtocol + "i.iabout.kr/w/"  +aboutCategoryAD1Banner.imageUrl +'" width="137" >';
				}
				else
				{
					//retValue += "<a href=\"" + rWingBanner.linkUrl +"\" target=\"_blank\"> <img src='" + locationProtocol + "is.iabout.kr/w/"  +rWingBanner.imageUrl +"'  width='80' height='250'  > </a>";					
					retValue += '		<img alt="'+ aboutCategoryAD1Banner.title  +'" src="'  + locationProtocol + "is.iabout.kr/w/"  +aboutCategoryAD1Banner.imageUrl +'" width="137" >';
				}
				
				retValue += '	</a> ';
				retValue += '</div>	';			
			}
			//}
		//}
		
		retValue += '<div class="outline btm"></div> ';
		retValue += '</div> ';
		retValue += '</div>   ';
		// 서브 카테 끝
		return retValue;		
}

TopHeader.getAboutCategoryAD1Banner = function(categoryCode)
{
	if(typeof(gAboutCategoryAD1Banner) != 'undefined' && gAboutCategoryAD1Banner.length > 0)
	{
		for(i = 0; i< gAboutCategoryAD1Banner.length ; i++)
		{
			if(gAboutCategoryAD1Banner[i].categoryCode == categoryCode)
			{
				return gAboutCategoryAD1Banner[i];
			}
		}
	}
	return null;
}


TopHeader.getLinkUrl = function(categoryCode)
{
		//alert(categoryCode.substring(2,6));
		linkType = categoryCode.substring(0,2);
		if( linkType == "25")
		{
			retValue = departmentPage + categoryCode;
		}
		else if( linkType == "18")
		{
			retValue = luxuryPage + categoryCode;
		}
		else if( linkType == "20")
		{
			retValue = overSeaPage + categoryCode;
		}			
		else
		{
			if(categoryCode.substring(2,8) == "000000")
			{
				// 대분류 카테고리일경우 cpp로 이동
				retValue = cppPage + categoryCode;
			}
			else
			{
				// 아니면 listing page로 이동..
				retValue = lpPage + categoryCode;
			}
		}
		return retValue;
}

