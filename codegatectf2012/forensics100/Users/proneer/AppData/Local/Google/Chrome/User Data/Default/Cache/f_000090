        $("#htxtHeaderSearch").val(GetUrlParamVal("keyword")); 	// 추가(2010.11.11) // [&#50939] 검색시 [웻] 으로 보이는 현상등으로 인해서 이곳에서 셋팅을 해준다
    }
}


function headerPbpBanner() {  };

headerPbpBanner.index = 0;
headerPbpBanner.maxLength = 0;

headerPbpBanner.getRand = function(n) {
		var r = Math.round(Math.random()*n-0.5);
					if ( r < 0 ) { r=0; } return r;
}

headerPbpBanner.pbpInit = function() {
	
	if(typeof(aboutPbpBanners) == "undefined")
	{
		return;
	}
	
	randPosition = headerPbpBanner.getRand(aboutPbpBanners.length);
	
	
	if(headerType == "H")
	{
		headerPbpBanner.maxLength = 8;
	}
	else
	{
		headerPbpBanner.maxLength = 7;		
	}
	
	headerPbpBanner.movePbpBanner(randPosition);
	
}


headerPbpBanner.clickPbpBtn = function(movePoint) {
	headerPbpBanner.movePbpBanner(movePoint);
}

headerPbpBanner.movePbpBanner = function(movePoint)
{
	headerPbpBanner.index =  headerPbpBanner.index + movePoint;
	
	if(headerPbpBanner.index < -1)
	{
		headerPbpBanner.index = aboutPbpBanners.length -1;
	}
	else if( headerPbpBanner.index > aboutPbpBanners.length)
	{
		headerPbpBanner.index = 0;
	}
	var curPos = headerPbpBanner.index;
	var pbpListHtml = "<ul>";
	var maxLength = 0;
	if(headerPbpBanner.maxLength >= aboutPbpBanners.length)
	{
		maxLength = aboutPbpBanners.length;
	}
	else
	{
		maxLength =	headerPbpBanner.maxLength;	
	}
		
	for(i = 0 ; i< maxLength ;i++)
	{
		if(curPos < 0)
		{
			curPos = aboutPbpBanners.length -1;
		}
		else if( curPos >= aboutPbpBanners.length)
		{
			curPos = 0;
		}
		var linkUrl = 	aboutPbpBanners[curPos].linkUrl;
		if(linkUrl.indexOf("?")>0)
		{
			linkUrl += "&viewCode=0270020" + headerPbpBanner.fillZeroNum((curPos + 1),2);	
		}
		pbpListHtml += '<li><a href="'+ linkUrl +'"  target="_blank" >';
		var imgUrl = aboutPbpBanners[curPos].imageUrl1.replace('http://','');
		if(locationProtocol == "https://")
		{
			imgUrl = imgUrl.replace("i.iabout.kr","is.iabout.kr");
		}
		
		pbpListHtml += '<img src="'+ locationProtocol + imgUrl +'" alt="'+ aboutPbpBanners[curPos].title1 +'"  width="88px" height="23px" />';
		pbpListHtml += '</a></li>';
		curPos++;
	}
	pbpListHtml += "</ul>";
	
	$("div.bnrRateArea > dl > dd ").html(pbpListHtml);
}

headerPbpBanner.fillZeroNum = function(num,len) {
				var temp = num.toString();
				var length = len - temp.length;
				var ret = "";
				for (idx = 0; idx < length; idx++) {
					ret += "0"
				}
				ret += temp;
				return ret;
			}
	
	


	$(document).ready(function() {

				if(headerType != "S")
				{
					// 코너링크 바인딩.     
					setTimeout(function() {
						// 코너링크 바인딩
						$("div#hsz_inner.hsz_inner div.cornerLink").html(TopHeader.getCornerList());
						// 코너링크 이벤트 처리
						$("div#hsz_inner.hsz_inner div.cornerLink ul li a").each(function(index) {
							$(this).mouseleave(function() {
								if (gMenuLocation == '' || $(this).attr("class").indexOf(gMenuLocation) == -1) {
									var className = $(this).attr("class").replace("on", "");
									$(this).attr("class", className);
								}
	
							});
							$(this).mouseover(function() {
								if (gMenuLocation == '' || $(this).attr("class").indexOf(gMenuLocation) == -1) {
									var className = $(this).attr("class") + "on";
									$(this).attr("class", className);
								}
							});
	
						}
					);
						// 오늘의 쿠폰 랜더링
						$("div#hsz_inner.hsz_inner div.tCoupon").html(TopHeader.getCouponList());
	
						// 네비게이션 전체보기
						$(".cateList .cateN .catekeyList .in-line").append(TopHeader.getTotalCateNavi());
						// 이 메서드는 design.js에 있음. 카테고리 전체 보기 
						mcategoryKeyword();
	
	
					}, 5);
	
	
					// 네비게이션바 바인딩
	
	
					setTimeout(function() {
	
						$(".cateList .hotcateList .hcate:eq(0)").append(TopHeader.getHotcateList(0));
						$(".cateList .hotcateList .hcate:eq(1)").append(TopHeader.getHotcateList(1));
						$(".cateList .hotcateList .hcate:eq(2)").append(TopHeader.getHotcateList(2));
						$(".cateList .hotcateList .hcate:eq(3)").append(TopHeader.getHotcateList(3));
						$(".cateList .hotcateList .hcate:eq(4)").append(TopHeader.getHotcateList(4));
	
						// 최상단 노드 마우스 오버 이벤트		
						$(".hsz_in .hotcateList div.hcate").mouseover(function() {
							var tempClass = $(this).attr("class");
	
							$(this).addClass("cateOn");
							$(this).css("z-index", "100");
							$("h3", this).addClass("on");
							$("div.in-line", this).css("z-index", "100");
							$("div.in-line", this).css("display", "block");
							//$("div.in-line .box", this).css("visibility","visible");
							//$(".box", this).css("visibility","visible");
							//$($(this).attr("class"));
	
						});
	
						// 최상단 노드 마우스 아웃 이벤트
						$(".hsz_in .hotcateList div.hcate").mouseleave(function() {
							var tempClass = $(this).attr("class");
	
							$(this).removeClass("cateOn");
							$(this).css("z-index", "0");
							$("h3", this).removeClass("on");
							$("div.in-line", this).css("z-index", "0");
							$("div.in-line", this).css("display", "none");
							//$("div.in-line div.catekey-sub div.cateSList-sub", this).css("visibility","");
						});
	
						// li이벤트 처리
						$(".hsz_in .hotcateList div.hcate div.box ul li").mouseover(function() {
							$(this).addClass("mhover");
						});
	
						$(".hsz_in .hotcateList div.hcate div.box ul li").mouseleave(function() {
							$(this).removeClass("mhover");
						});
					}, 500);
	
					// 우측배너 바인딩
					setTimeout(function() {
						if (typeof (aboutHeaderBanner) == 'undefined') return;
						if (aboutHeaderBanner.length > 0) {
							gAboutTic = new Date().getSeconds() % aboutHeaderBanner.length;
						}
						HeaderCorner.BindRightBanner();
					}, 2500);
	
					// pbp Banner 바인딩
					if (typeof (aboutPbpBanners) != "undefined") {
						setTimeout(function() {
							headerPbpBanner.pbpInit();
	        
							//if	
							if ((headerType == "H" && aboutPbpBanners.length > 8) || (headerType != "H" && aboutPbpBanners.length > 7)) {
								$("div.bnrRateArea div.btn-pn a.btn_prev").css("cursor", "pointer");
								$("div.bnrRateArea div.btn-pn a.btn_next").css("cursor", "pointer");
								$("div.bnrRateArea div.btn-pn a.btn_prev").click(function() {
									headerPbpBanner.clickPbpBtn(1);
								}
						);
	        
								$("div.bnrRateArea div.btn-pn a.btn_next ").click(function() {
									headerPbpBanner.clickPbpBtn(-1);
								}
						);
							}
	        
						}, 1000);
					}
			}
	})

	
	function hotcateOver(obj)
	{
		$("#"+ obj).css("visibility","visible");
		$("#"+ obj).addClass("mhover");
		$("a:first",$("#"+ obj).parent()).addClass("on");		
	}
	
	function hotcateOut(obj)
	{
		$("#"+ obj).css("visibility","");
		//$("#"+ obj).attr("class","mhover");
		$("#"+ obj).removeClass("mhover");
		$("a:first",$("#"+ obj).parent()).removeClass("on");		
	}
	
	function logoScreensaverClick()
	{
		window.open("http://www.about.co.kr/popup/aboutScreenSaverDownload.htm", "aboutScreenSaverDownload", "width=330, height=355, status=no, resizable=no, scrollbars=no ");
	}
	
	String.prototype.startsWith = function(str){     return (this.indexOf(str) === 0); } 