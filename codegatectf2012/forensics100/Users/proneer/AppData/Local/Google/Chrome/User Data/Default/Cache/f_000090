///////////////// Firefox 버그
// FireFox suggest버그 수정을 위한 플러그 코드 
(function($) {
	$.FFInputBoxFix = {
		startWatch: function() {
			 focusFunc();
		}	
	}
	
	
	$.fn.FFInputBoxFix = function(suggestIframeId) {
		if (jQuery.browser.mozilla == false)
			return false;

		var db = "";
		var keyEventId = null;
		var inputTextId = $(this).attr("id");
		var suggestId = suggestIframeId;
		$(this).bind("focus", focusFunc);
		$(this).bind("blur", blurFunc);

		this.keyEventCheck = null;
		focusFunc();

		function focusFunc() {
			if (keyEventId != null)
				clearTimeout(keyEventId);
				
			keyEventId = setTimeout(function() {
				checkInputText()
			}, 250);

		}
		function blurFunc() {
			if (keyEventId != null)
				clearTimeout(keyEventId);
		}
		
		function checkInputText() {
			//console.log(new Date()+"------"+ 11111);
			inputSearchKeywordBox = $("#"+inputTextId);
			if (db != $(inputSearchKeywordBox).val() && $(inputSearchKeywordBox).val() != "") {
				var suggestObj = $("#"+suggestId).contents();
				if ($('div.autoComp li a.suggestHover', suggestObj).length <= 0) {
					
					// 트리거 발생..
					var event = jQuery.Event("keyup");
					event.keyCode = -1;
					$(inputSearchKeywordBox).trigger(event);
					
					//db에다가 저장..
					db = $(inputSearchKeywordBox).val();
				}
			}
			// 타이머 초기화
			clearTimeout(keyEventId);
			// 재귀호출
			keyEventId = setTimeout(
						function() {
							checkInputText();
						}, 250);
		}	
	}	
})(jQuery);




