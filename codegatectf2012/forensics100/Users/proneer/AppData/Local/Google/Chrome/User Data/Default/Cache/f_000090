	$(".topAdv .inner .topAdvLayer").hide();	
}

HeaderCorner.setCookie = function( name, value, expiredays ) 
{ 
	var todayDate = new Date(); 
	todayDate.setDate( todayDate.getDate() + expiredays ); 
	todayDate.setHours(0,0,0,0);
	if(typeof(expiredays) == 'undefined' || expiredays == null || expiredays == "")
	{
		document.cookie = name + "=" + escape( value ) + ";path=/;domain=about.co.kr;";
		
	}
	else
	{
		document.cookie = name + "=" + escape( value ) + ";path=/;domain=about.co.kr;expires=" + todayDate.toGMTString() + ";" 
	}
		
		
}

HeaderCorner.getCookie = function( name ){
	var nameOfCookie = name + "=";
	var x = 0;
	while ( x <= document.cookie.length )
	{
		var y = (x+nameOfCookie.length);
		if ( document.cookie.substring( x, y ) == nameOfCookie ) {
		if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )
		endOfCookie = document.cookie.length;
		return unescape( document.cookie.substring( y, endOfCookie ) );
	}
	x = document.cookie.indexOf( " ", x ) + 1;
	if ( x == 0 )
		break;
	}
	return "";
}

HeaderCorner.OpenFlowerGarden = function() {
	var url = "http://finding.about.co.kr/Corner/FlowerGarden/Main.aspx";
	window.open(url, "", "toolbar=no,location=yes,directories=no,status=no,menubar=no,scrollbars=no, resizable=no,copyhistory=no ,width=1188 height=880, left=10,top=10");
}

HeaderCorner.LMoveRightBanner = function(){
	gAboutTic--;
	if(gAboutTic <= -1)
		gAboutTic = aboutHeaderBanner.length -1 ;
	HeaderCorner.BindRightBanner();
}

HeaderCorner.RMoveRightBanner = function(){
	gAboutTic++;
	if(gAboutTic >= aboutHeaderBanner.length)
		gAboutTic = 0;
	HeaderCorner.BindRightBanner();
}
HeaderCorner.BindRightBanner = function(){
		var bindHtml ="";
		if(typeof(aboutHeaderBanner) != 'undefined' && aboutHeaderBanner.length > 0)
		{
			if(aboutHeaderBanner[gAboutTic].imageType == "0")// 이미지 타입
			{
				bindHtml += ' <a href="'+ aboutHeaderBanner[gAboutTic].linkUrl +'" target="_self">';
				bindHtml += '	<img width="200px" height="70px" src="'+ locationProtocol + aboutHeaderBanner[gAboutTic].imageUrl.replace('http://','') + '" alt="'+''+'" />';
				bindHtml += '	</a>';
			} 
			else if(aboutHeaderBanner[gAboutTic].imageType == "1")// 플래시타입
			{
				bindHtml += '	<div id="gnbEventBannerSwf">\n';
				bindHtml += '	    <script type="text/javascript"> \n';
				bindHtml += '     var hbflashvars={ \n';
				bindHtml += '     // link  :  배너 클릭 시 이동할 링크 url \n';
				bindHtml += '     // target : 배너 클릭 시 이동할 링크 target \n';
				bindHtml += '     link : \''+ aboutHeaderBanner[aboutTic].linkUrl +'\', \n';
				bindHtml += '     target : \'_blank\' \n';
				bindHtml += '     }; \n';
				bindHtml += "     embedFlash('" + locationProtocol + aboutHeaderBanner[aboutTic].imageUrl.replace('http://','')  + "', 'gnbEventBannerSwf', '100%', '100%', \"opaque\", hbflashvars); \n";
				bindHtml += '    <\/script>\n';
				bindHtml += '   <\/div>\n';
			} 
			else
			{
				var hBannerHtml =  aboutHeaderBanner[gAboutTic].htmlContents.replace("[%=이미지%]", locationProtocol + aboutHeaderBanner[gAboutTic].imageUrl.replace('http://','') );			
				hBannerHtml = hBannerHtml.replace("[%=링크%]", aboutHeaderBanner[gAboutTic].linkUrl );
				bindHtml += hBannerHtml;
			}
			if(aboutHeaderBanner.length>1)
			{
				bindHtml += '<div class="btn_rolling">';
				bindHtml += '<a onclick="HeaderCorner.LMoveRightBanner()" style="cursor:pointer;" class="prev" title="이전배너"></a>';
				bindHtml += '	<a onclick="HeaderCorner.RMoveRightBanner()" style="cursor:pointer;" class="next" title="다음배너"></a>';
				bindHtml += '</div>';
				
			}
			$("#gnbEventBannerDiv").html(bindHtml);
		}	
}

