<!--SEO Dynamic link remover Script -->
<script type="text/javascript">
    document.body.className=document.body.className+' hideVisibleSEOLinks';
    var pageURL=window.location.href;
    var siteXML;
    var testRegEx = new RegExp("(http|https)://(.+)\.emc\.com/(.+)$");
    function checkSEOLinks(el,ix,flipTagOrder){
    var elA=el.up(null,0);
    if (elA.tagName=='A' || elA.tagName=='a') {
        var testURL=elA.href;
        if (testRegEx.test(testURL)) {
            testURLsplit=testURL.split('.emc.com');
            testURL=testURLsplit[1];
            if (!siteXML.transport.responseText.match(testURL)) {
                elA.replace(elA.innerHTML.stripTags());
            }
        }
    }
    var thisA=el.select('a').each(function(elA,ixA) {
        var testURL=elA.href;
        if (testRegEx.test(testURL)) {
            testURLsplit=testURL.split('.emc.com');
            testURL=testURLsplit[1];
            if (!siteXML.transport.responseText.match(testURL)) {
                el.replace(el.innerHTML.stripTags());
            }
        }
    });
    }
    Event.observe(window,'load',function() {
        siteXML=new Ajax.Request('/seo-sitemap.xml',{
            method:'get',
            onSuccess:function(x){
                $$('.themeSEOLinks').each(function(el,ix) { checkSEOLinks(el,ix);});
                $$('.themeSEOLinksVisible').each(function(el,ix) { checkSEOLinks(el,ix);});
        document.body.className=document.body.className.replace('hideVisibleSEOLinks','');
            },
            onFailure:function(){
                document.body.className=document.body.className.replace('hideVisibleSEOLinks','');
            }
        });
    });
</script>
<!--SEO Dynamic link remover Script END-->






