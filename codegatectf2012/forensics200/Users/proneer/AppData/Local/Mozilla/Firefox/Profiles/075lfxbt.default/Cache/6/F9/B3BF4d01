<script src="assets/js/eventCatalog.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript" charset="utf-8">
	var markScheduled = null;
	var markCleared = null;
	$(function(){
		initResultTableActions("#eventCatalog", {
			advancedSearchOffText: 'Hide Advanced Options',
			searchInputMessage: 'Keyword Search',
			searchURL: '/US12/scheduler/eventcatalog/eventCatalogResults.do',
			exhibitorDetailURL: '/US12/scheduler/eventcatalog/exhibitorDetail.do',
			sessionDetailURL: '/US12/scheduler/eventcatalog/sessionDetail.do',
			demoDetailURL: '/US12/scheduler/eventcatalog/demoDetail.do',
			showDetailLinkText: 'Show Full Detail',
			hideDetailLinkText: 'Hide Detail'
		});
		
		// when the search is clicked
		$("#searchForm").submit(function(){
			var sessionSpecificSelected = false;
			$("#sessionSpecific select").each(function(){
				if($(this).attr("selectedIndex") > 0) {
					sessionSpecificSelected = true;
				}
			});
			
			var exhibitorSpecificSelected = false;
			$("#exhibitorSpecific select").each(function(){
				if($(this).attr("selectedIndex") > 0) {
					exhibitorSpecificSelected = true;
				}
			});
			
			if (exhibitorSpecificSelected && !sessionSpecificSelected) selectExhibitorTab();
			if (!exhibitorSpecificSelected && sessionSpecificSelected) selectSessionTab();
			if (exhibitorSpecificSelected && sessionSpecificSelected) selectAllTab();
		});
	});
	function toggleDetail(el){
		$(el).closest('tr').trigger('toggleDetail');
	}
	function hideDetail(el){
		$(el).closest('tr').trigger('toggleDetail', [false]);
	}
	function changeSessionStatus(){
		if(markScheduled != null){
			$($currentEditItem).addClass('scheduled');
			markScheduled = null;
		}
		if(markCleared != null){
			$($currentEditItem).removeClass('scheduled');
			markCleared = null;
		}
	}
	/*
		type - String, the item type clicked on (currently "session", "exhibitor" or "demo")
		itemId - Integer, the id of the item clicked on
		action - String, "add" or "remove"

		The ajax should return a string of "success", "logout" or an error to be displayed
	 */
	function shoppingCartToggle(type, itemId, action, callback){
		if('session' == type )
		SchedulerAjax.addOrRemoveSessionFromPersonInterest(action, itemId, callback);
		else
			SchedulerAjax.addOrRemovePersonFromExhibitorLeads(action, itemId, callback);
	}

	var sessionsToChange = new Array();
	function toggleAjax(sessionID){
		if(!$("#session_" + sessionID).find("img.status").hasClass("scheduled")){
			addAjax(sessionID);
		}
		else{
			removeAjax(sessionID);
		}
	}
	
	function addAjax(sessionID) {
		/* Passing in null means we do not have a session remove, the user is adding a session without another session already scheduled for this slot.*/
		SchedulerAjax.addUserToSession(sessionID, null,sessionCallback);
		sessionsToChange.unshift(sessionID);
	}

	function removeAjax(sessionID) {
		SchedulerAjax.removeUserFromSession(sessionID, sessionCallback);
		sessionsToChange.unshift(sessionID);
	}

	function sessionCallback(retval) {
		var currentSession = sessionsToChange.pop();
		if(retval == "success"){
			if(currentSession != null && currentSession != undefined){
				var currentIcon = $("#session_" + currentSession).find("img.status");
				var addClass = "scheduled";
				var removeClass = "schedule";
				currentIcon.attr("title", 'Remove from My Schedule');
				if(currentIcon.hasClass("scheduled")){
					addClass = "schedule";
					removeClass = "scheduled";
					currentIcon.attr("title", 'Add to My Schedule');
				}
				currentIcon.removeClass(removeClass);
				currentIcon.addClass(addClass);
				
			}
		}
		else{
			alert(retval);
		}
	}
	
	$(function (){
		$("#breadcrumb").html('');
		var html = '<a class="section" href="http://www.rsaconference.com/events/index.htm">Events</a><span class="blue">&raquo;</span>';
		html += '<a class="section" href="http://www.rsaconference.com/events/2012/usa/index.htm">USA 2012</a><span class="blue">&raquo;</span>';
		html += '&nbsp;&nbsp;&nbsp;Event Catalog';
		html += '</div><br/>';
		
		$('#breadcrumb').html(html);
	});
		
		
	
</script>
