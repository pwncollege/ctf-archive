	.formatTableWidth,.formatHeaderTable{
		width:350px;
	}
</style>













  





<script type="text/javascript">
/* <![CDATA[ */
var currentProfileValues = new Array();

function hasClass(el,cls) {
 if(el){
  return (el.className.match(cls) != null);
 }
}

function addClass(el,cls) {
 if(el){
  if (!hasClass(el,cls)) el.className += ' '+cls;
 }
}

function removeClass(el,cls) {
 if (el) {
  var reg = new RegExp(cls, 'g');
  el.className = el.className.replace(reg,'');
 }
}


function change_profileItem_10006(display) {
 var profileItem_10006_tr = document.getElementById("profileItem_10006_tr");
 var profileItem_10006 = document.getElementById("profileItem_10006");
 var toValidateClass = "";

 if(display == 'show' || display == null){
   removeClass(profileItem_10006_tr, 'hide');
   if(profileItem_10006) profileItem_10006.className = toValidateClass;
} else{
  removeClass(profileItem_10006_tr, 'show');
  addClass(profileItem_10006_tr, 'hide');
   if(profileItem_10006) profileItem_10006.className = 'ignore';
 }

 var profileItem_10006_select_one = document.getElementById("profileItem_10006_select_one");
 var profileItem_10006_10010 = document.getElementById("profileItem_10006_10010");
 var profileItem_10006_10011 = document.getElementById("profileItem_10006_10011");
 var profileItem_10006_10012 = document.getElementById("profileItem_10006_10012");
 var allRadioANDCheckValues10006 = new Array();
 var specifyValues = new Array();
 var profileItemName = 'profileItem_10006'


 if (allRadioANDCheckValues10006.length > 0){
   for(var i = 0; i < allRadioANDCheckValues10006.length; i++){
     var checkBoxValue = document.getElementById(profileItemName + '_' + allRadioANDCheckValues10006[i])
     if (checkBoxValue != null){
       if (typeof(ifOtherSpecify_tr) != 'undefined' && checkBoxValue.checked && specifyValues[checkBoxValue.value]){
         ifOtherSpecify_tr.className='show';
       }else if (!checkBoxValue.checked && specifyValues[checkBoxValue.value]){ 
         ifOtherSpecify_tr.className='hide';
       }
     }
   }
 } else {
   var selectItem = document.getElementById(profileItemName);
   if (selectItem != null && typeof(ifOtherSpecify_tr) != 'undefined'){
     ifOtherSpecify_tr.className='hide';
     for (var i = 0; i < selectItem.options.length; i++){
       if (selectItem.options[i].selected && specifyValues[selectItem.options[i].value])
         ifOtherSpecify_tr.className='show'; 
     }
   }
 }

 if(display == 'show') {
   profileItem_10006_select_one.selected = true;
   profileItem_10006.className = toValidateClass;
   profileItem_10006_10010.selected = false;
   profileItem_10006_10011.selected = false;
   profileItem_10006_10012.selected = false;
 }
 if(display == 'hide') {
   profileItem_10006_select_one.selected = true;
   profileItem_10006.className = 'ignore';
   profileItem_10006_10010.selected = false;
   profileItem_10006_10011.selected = false;
   profileItem_10006_10012.selected = false;
 }
}

