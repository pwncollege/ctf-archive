function change_profileItem_10541(display) {
 var profileItem_10541_tr = document.getElementById("profileItem_10541_tr");
 var profileItem_10541 = document.getElementById("profileItem_10541");
 var toValidateClass = "";

 if(display == 'show' || display == null){
   removeClass(profileItem_10541_tr, 'hide');
   if(profileItem_10541) profileItem_10541.className = toValidateClass;
} else{
  removeClass(profileItem_10541_tr, 'show');
  addClass(profileItem_10541_tr, 'hide');
   if(profileItem_10541) profileItem_10541.className = 'ignore';
 }

 var profileItem_10541_select_one = document.getElementById("profileItem_10541_select_one");
 var profileItem_10541_11742 = document.getElementById("profileItem_10541_11742");
 var profileItem_10541_13840 = document.getElementById("profileItem_10541_13840");
 var profileItem_10541_12741 = document.getElementById("profileItem_10541_12741");
 var profileItem_10541_12124 = document.getElementById("profileItem_10541_12124");
 var profileItem_10541_12720 = document.getElementById("profileItem_10541_12720");
 var profileItem_10541_12130 = document.getElementById("profileItem_10541_12130");
 var profileItem_10541_14000 = document.getElementById("profileItem_10541_14000");
 var profileItem_10541_12129 = document.getElementById("profileItem_10541_12129");
 var profileItem_10541_12380 = document.getElementById("profileItem_10541_12380");
 var profileItem_10541_13720 = document.getElementById("profileItem_10541_13720");
 var profileItem_10541_12132 = document.getElementById("profileItem_10541_12132");
 var profileItem_10541_12133 = document.getElementById("profileItem_10541_12133");
 var profileItem_10541_13240 = document.getElementById("profileItem_10541_13240");
 var profileItem_10541_12126 = document.getElementById("profileItem_10541_12126");
 var profileItem_10541_12123 = document.getElementById("profileItem_10541_12123");
 var profileItem_10541_12740 = document.getElementById("profileItem_10541_12740");
 var profileItem_10541_12125 = document.getElementById("profileItem_10541_12125");
 var profileItem_10541_12134 = document.getElementById("profileItem_10541_12134");
 var profileItem_10541_12600 = document.getElementById("profileItem_10541_12600");
 var profileItem_10541_13721 = document.getElementById("profileItem_10541_13721");
 var profileItem_10541_13841 = document.getElementById("profileItem_10541_13841");
 var profileItem_10541_12135 = document.getElementById("profileItem_10541_12135");
 var profileItem_10541_12131 = document.getElementById("profileItem_10541_12131");
 var profileItem_10541_12620 = document.getElementById("profileItem_10541_12620");
 var allRadioANDCheckValues10541 = new Array();
 var specifyValues = new Array();
 var profileItemName = 'profileItem_10541'


 if (allRadioANDCheckValues10541.length > 0){
   for(var i = 0; i < allRadioANDCheckValues10541.length; i++){
     var checkBoxValue = document.getElementById(profileItemName + '_' + allRadioANDCheckValues10541[i])
     if (checkBoxValue != null){
       if (typeof(ifOtherSpecify_tr) != 'undefined' && checkBoxValue.checked && specifyValues[checkBoxValue.value]){
         ifOtherSpecify_tr.className='show';
       }else if (!checkBoxValue.checked && specifyValues[checkBoxValue.value]){ 
         ifOtherSpecify_tr.className='hide';
       }
     }
   }
 } else {
   var selectItem = document.getElementById(profileItemName);
   if (selectItem != null && typeof(ifOtherSpecify_tr) != 'undefined'){
     ifOtherSpecify_tr.className='hide';
     for (var i = 0; i < selectItem.options.length; i++){
       if (selectItem.options[i].selected && specifyValues[selectItem.options[i].value])
         ifOtherSpecify_tr.className='show'; 
     }
   }
 }

 if(display == 'show') {
   profileItem_10541_select_one.selected = true;
   profileItem_10541.className = toValidateClass;
   profileItem_10541_11742.selected = false;
   profileItem_10541_13840.selected = false;
   profileItem_10541_12741.selected = false;
   profileItem_10541_12124.selected = false;
   profileItem_10541_12720.selected = false;
   profileItem_10541_12130.selected = false;
   profileItem_10541_14000.selected = false;
   profileItem_10541_12129.selected = false;
   profileItem_10541_12380.selected = false;
   profileItem_10541_13720.selected = false;
   profileItem_10541_12132.selected = false;
   profileItem_10541_12133.selected = false;
   profileItem_10541_13240.selected = false;
   profileItem_10541_12126.selected = false;
   profileItem_10541_12123.selected = false;
   profileItem_10541_12740.selected = false;
   profileItem_10541_12125.selected = false;
   profileItem_10541_12134.selected = false;
   profileItem_10541_12600.selected = false;
   profileItem_10541_13721.selected = false;
   profileItem_10541_13841.selected = false;
   profileItem_10541_12135.selected = false;
   profileItem_10541_12131.selected = false;
   profileItem_10541_12620.selected = false;
 }
 if(display == 'hide') {
   profileItem_10541_select_one.selected = true;
   profileItem_10541.className = 'ignore';
   profileItem_10541_11742.selected = false;
   profileItem_10541_13840.selected = false;
   profileItem_10541_12741.selected = false;
   profileItem_10541_12124.selected = false;
   profileItem_10541_12720.selected = false;
   profileItem_10541_12130.selected = false;
   profileItem_10541_14000.selected = false;
   profileItem_10541_12129.selected = false;
   profileItem_10541_12380.selected = false;
   profileItem_10541_13720.selected = false;
   profileItem_10541_12132.selected = false;
   profileItem_10541_12133.selected = false;
   profileItem_10541_13240.selected = false;
   profileItem_10541_12126.selected = false;
   profileItem_10541_12123.selected = false;
   profileItem_10541_12740.selected = false;
   profileItem_10541_12125.selected = false;
   profileItem_10541_12134.selected = false;
   profileItem_10541_12600.selected = false;
   profileItem_10541_13721.selected = false;
   profileItem_10541_13841.selected = false;
   profileItem_10541_12135.selected = false;
   profileItem_10541_12131.selected = false;
   profileItem_10541_12620.selected = false;
 }
}

