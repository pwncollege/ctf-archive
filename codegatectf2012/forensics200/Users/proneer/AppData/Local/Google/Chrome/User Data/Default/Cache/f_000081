<script type="text/javascript">
	// CategoryNavigationBar.ascx	
	$(document).ready(function() {

	//마지막에 last class 를 설정해준다.
	$("table[id^='NavigationBar_table']").each(function() {
		$("td[id^='NavigationBar_tdLast']:last", this).addClass("last");
	});
});

// 네비게이션바 마우스 오버시 나오는 레이어 보이기/숨기기
function toggleNavigationBarChildLayer(option, obj) {
	$div_ChildLayer = $(obj).parent().find("#divNavigationBar_ChildLayer");

	if (option == 'show') {
		$div_ChildLayer.show();
	}
	else if (option == 'hide') {
		$div_ChildLayer.hide();
	}
}
</script>

