<script type="text/javascript">
	// 네비게이션바 마우스 오버시 나오는 레이어 보이기/숨기기
	function toggleCategoryBoxNavigationBarChildLayer(option, id) {
		$div_ChildLayer = $("#" + id);

		if (option == 'show') {
			$div_ChildLayer.show();
		}
		else if (option == 'hide') {
			$div_ChildLayer.hide();
		}
	}

	// 전역변수
	function toggleCategoryBoxNavigationBarChildLayer_ByAjaxCall(option, id, CategoryLevel) {

		var $div_ChildLayer = $("#" + id);
		if ($div_ChildLayer.attr("hasData") != "true") {
			$.ajax({
				url: "/Search/CategoryBoxSRPGroupByAjaxResult.aspx", /* BC2658 제휴사 lp/srp ajaxCall 주소를 바인드 */
				data: "keyword=" + gCheckUndefined(gDefaultState.Keyword)
                + "&displayCategories=" + gCheckUndefined(gDefaultState.DisplayCategories)
                + "&pageIndex=" + gCheckUndefined(gDefaultState.PageIndex)
                + "&pageSize=" + gCheckUndefined(gDefaultState.PageSize)
                + "&FindingType=" + gCheckUndefined(gDefaultState.FindingType)
                + "&VItemAttrs=" + gCheckUndefined(gDefaultState.VItemAttrs)
                + "&BrandIDs=" + gCheckUndefined(gDefaultState.BrandIDs)
                + "&MakerIDs=" + gCheckUndefined(gDefaultState.MakerIDs)
                + "&KeywordsExclude=" + gCheckUndefined(gDefaultState.KeywordsExclude)
                + "&MinVItemPrice=" + gCheckUndefined(gDefaultState.MinVItemPrice)
                + "&MaxVItemPrice=" + gCheckUndefined(gDefaultState.MaxVItemPrice)
                + "&ViewCode=" + gCheckUndefined(gDefaultState.ViewCode)
                + "&KeywordsInclude=" + gCheckUndefined(gDefaultState.KeywordsInclude)
                + "&MallIDs=" + gCheckUndefined(gDefaultState.MallIDs)
                + "&cornerType=" + gCheckUndefined(gDefaultState.CornerType)
                + "&categoryLevel=" + CategoryLevel
                ,
				async: true,
				success: function(resultHtml) {
					$div_ChildLayer.html(resultHtml);
					$div_ChildLayer.show();
					$div_ChildLayer.attr("hasData", "true");
					OS2.Application.CategoryBoxSRP.bindCategoryClickEvent($div_ChildLayer);  // 이벤트를 걸어준다
				},
				error: function(xhr) {
					commonAjaxErrorHandler(xhr.status, xhr.statusText);
				}
			});
		}
		else {
			$div_ChildLayer.show();
		}
	}
</script>

