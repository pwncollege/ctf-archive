	// Ajax Call 로 Rendering 되는 경우를 고려해서 ready 로 처리하지 않음
	function toggleDebugInfo(obj) {
		var $divContent = $(obj).siblings().find("div");
		$divContent.toggleClass("DebugInfoContent");
