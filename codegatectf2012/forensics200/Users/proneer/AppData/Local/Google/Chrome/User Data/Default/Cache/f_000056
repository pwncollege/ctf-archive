<script type="text/javascript" src="http://adw.gomtv.com/ads.gom?dispid=ban@gomtv_web_sch"></script>
<script type="text/javascript">
searchbg_state = 1;
if (searchImg != null)
	searchImgJson = eval('(' + searchImg + ')');

if (searchImgJson[0].imgurl){
	document.getElementById('searchbgimg').style.background = "url('" + searchImgJson[0].imgurl + "')  no-repeat center";
}

function goTopSearch()
{
	var tf = document.topSearchForm;

	if (searchImgJson[0].targeturl && searchbg_state) {
		var str="<iframe name='runframe' id='runframe' src='"+searchImgJson[0].targeturl+"' width='0' height='0'></iframe>";
		document.getElementById('hiddenlyr').innerHTML=str;
	} else {
		if( tf.key.value.length > 100 ) {
			alert('검색어를 정확하게 입력하세요.');
			tf.key.value = '';
			tf.key.focus();
			return;
		}

		if( tf.key.value == '검색어/채널번호를 입력하세요!' ) {
			alert('검색어를 입력하세요.');
			return;
		}

		if( chk_empty(tf.key.value) == true ) {
			alert('검색어를 입력하세요.');
			return;
		}

		tf.action = 'http://search.gomtv.com/search.gom';
		tf.submit();
	}
}
</script>

