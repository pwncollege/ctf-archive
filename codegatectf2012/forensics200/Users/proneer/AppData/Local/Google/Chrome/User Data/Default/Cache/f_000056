<script language=javascript>
var searchImg;
var searchImgJson = Array('');

document.domain='gomtv.com';
window.name='GOMTV';

function chk_empty(str)
{
	if ( str.match(/\S/) == null || str.match(/\S/) == "" ) { return(true); }
	else { return(false);}
}

function goWord( word ) {
	document.location.href = 'http://search.gomtv.com/search.gom?key=' + word;
}
function blank_keyword()
{
	var tf = document.topSearchForm;
	if( tf.key.value == '검색어/채널번호를 입력하세요!' )
	{
		tf.key.value = '';
		tf.key.focus();
	}
}
function toggleOptions(){
	if (document.getElementById('select_web').style.display=='none'){
		document.getElementById('select_web').style.display='block';
	}
	else{
		document.getElementById('select_web').style.display='none';
	}
}
function selectSearchOption(value, option){
	document.getElementById('systype').value=value;
	document.getElementById('curOption').innerHTML=option;
	document.getElementById('select_web').style.display='none';
}
</script>
